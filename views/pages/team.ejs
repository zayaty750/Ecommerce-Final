<!DOCTYPE html>
<html lang="en">

<head>
  <%- include("../partials/Head.ejs") %>
</head>

<body>
  <div class="page d-flex">
    <%- include("../partials/sidebar_admin.ejs") %>
      <div class="content w-full">
        <!-- Start Head -->
        <%- include("../partials/Navbar_admin.ejs") %>
          <!-- End Head -->
          <h1 class="p-relative">Blankoo Team</h1>
          <div class="row ">
            <div class="btn-group d-flex justify-content-center  col-12 mb-5" role="group" aria-label="Basic outlined example">
              <a type="button" class="btn btn-outline-dark" href="/add-admin" >Add Admin</a>
              <a type="button" class="btn btn-outline-dark active" href="/team">View Team</a>
            </div>
          <div class="friends-page d-grid m-20 gap-20">
            <% user.forEach((User)=> { %>
              <% if(User.isAdmin==true){ %>
                <%- include('../partials/user_card', { User: User }) %>
                  <% } %>
                    <% }); %>
          </div>
      </div>
  </div>
</body>

</html>